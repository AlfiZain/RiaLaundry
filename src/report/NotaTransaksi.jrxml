	<queryString>
		<![CDATA[SELECT t.id_transaksi, t.total_harga, t.status_pembayaran, 
       dt.id_layanan, dt.jumlah, dt.total,
       p.nama_pelanggan,
       l.nama_layanan, l.harga,
       k.nama_karyawan
FROM transaksi t
INNER JOIN detail_transaksi dt ON t.id_transaksi = dt.id_transaksi
INNER JOIN pelanggan p ON t.id_pelanggan = p.id_pelanggan
INNER JOIN layanan l ON dt.id_layanan = l.id_layanan
INNER JOIN karyawan k ON t.id_karyawan = k.id_karyawan
WHERE t.id_transaksi = $P{id}]]>
	</queryString>
