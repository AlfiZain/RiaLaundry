	</parameter>
	<queryString>
		<![CDATA[SELECT k.id_komplain , k.id_transaksi, k.detail_komplain, k.tanggal_komplain,
p.nama_pelanggan FROM komplain k 
INNER JOIN transaksi t ON k.id_transaksi = t.id_transaksi 
INNER JOIN pelanggan p ON t.id_pelanggan = p.id_pelanggan
WHERE k.tanggal_komplain BETWEEN $P{tgl1} AND $P{tgl2}]]>
	</queryString>
