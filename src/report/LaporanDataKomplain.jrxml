	<subDataset name="Transaksi data set" uuid="ef987a8a-cc19-4dce-8669-8c204fa595e5">
		<parameter name="tgl1" class="java.util.Date">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="tgl2" class="java.util.Date">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT t.id_transaksi, p.nama_pelanggan, t.total_harga,
t.status_pembayaran, t.tanggal_transaksi, k.nama_karyawan FROM transaksi t 
INNER JOIN pelanggan p ON t.id_pelanggan = p.id_pelanggan 
INNER JOIN karyawan k ON t.id_karyawan = k.id_karyawan
WHERE t.tanggal_transaksi BETWEEN $P{tgl1} AND $P{tgl2}]]>
		</queryString>
		<field name="id_transaksi" class="java.lang.Integer">
