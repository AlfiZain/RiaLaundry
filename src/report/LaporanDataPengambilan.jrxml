	<queryString>
		<![CDATA[SELECT pa.id_pengambilan, pa.id_transaksi, pa.nama_pengambil, pa.tanggal_pengambilan, p.nama_pelanggan, 
t.status_pembayaran FROM pengambilan pa 
INNER JOIN transaksi t ON pa.id_transaksi = t.id_transaksi 
INNER JOIN pelanggan p ON t.id_pelanggan = p.id_pelanggan
WHERE pa.tanggal_pengambilan BETWEEN $P{tgl1} AND $P{tgl2}]]>
	</queryString>
